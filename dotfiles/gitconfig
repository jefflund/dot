[core]
    editor = vim
[color]
    ui = true
[alias]
    st = status -sb
    ci = commit
    br = branch
    co = checkout
    df = diff
    dfc = diff --cached
    l = log --oneline --decorate --name-status
    lg = log --oneline --decorate --name-status --graph
    lo = log --oneline
[push]
    default = current
