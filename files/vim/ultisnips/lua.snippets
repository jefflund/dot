snippet l
local $0
endsnippet

snippet fn
function ${1:name}($2)
	$0
end
endsnippet

snippet lfn
local function ${1:name}($2)
	$0
end
endsnippet

snippet rfn
function ${1:cls}:${2:name}($3)
	$0
end
endsnippet

snippet fori
for ${1:i}=${2:1},${3:n} do
	$0
end
endsnippet

snippet forv
for _, ${1:v} in ${2:i}pairs(${3:vals}) do
	$0
end
endsnippet
